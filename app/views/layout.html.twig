<!doctype html>
<html class="no-js" lang="nl">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

  <title>{{ app.title }}</title>
  <link rel="stylesheet" href="{{ app.request.basepath }}/css/normalize.css">
  <link rel="stylesheet" href="{{ app.request.basepath }}/css/all.css">
  <link rel="stylesheet" href="{{ app.request.basepath }}/css/custom.css">
  <script src="{{ app.request.basepath }}/js/modernizr-2.5.3-custom-1.js"></script>
  <script src="{{ app.request.basepath }}/js/jquery-1.7.2.min.js"></script>
  <script src="/js/portfolio-browser.js"></script>
  {% include 'layout.head.custom.html.twig' ignore missing %}
</head>
<body>

<div id="main-container" class="cf">
  <div class="cf" id="topbar">
    {% include 'topbar.html.twig' %}
  </div>
  {% block sidebar %}
  {{ render('/sidebar?browsing_code=' ~ block('browsing_code')) }}
  {% endblock %}
  <div id="main-content">
    {% block content %}{% endblock %}
  </div>
  {% block tagcloud %}
  {{ render('/tagcloud?browsing_code=' ~ block('browsing_code')) }}
  {% endblock %}
</div>

</body>
</html>
